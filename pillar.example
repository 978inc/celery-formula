celery:
  lookup:
    version: '4.0.2'
    log_dir: /var/log/celery
    broker: redis

  worker_queues:
    - name: 'notfast'
      opts: {batch_size: 5}
      concurrency: 1

    - name: 'superfast'
      opts: {batch_size: 100, prefetch_multiplier: 10}
      concurrency: 3

  config:
    worker:
      concurrency: 4
      batch_size: 10
      
    broker:
      url: 'redis://localhost:6379/0'
    
    imports: ['pprint.pprint', 'copy.copy']
    enable_utc: true
    timezone: 'UTC'
    result_backend: 'redis://localhost:6379/1'

